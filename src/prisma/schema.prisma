datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model TypeDoctor {
  id          Int      @id @default(autoincrement())
  name        String   @unique
  description String   @db.Text
  doctors     Doctor[] @relation("TypeDoctorDoctors")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Doctor {
  id          Int        @id @default(autoincrement())
  name        String
  typeDoctors TypeDoctor[] @relation("TypeDoctorDoctors")
}
