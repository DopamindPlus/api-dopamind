datasource db {
  provider = "mysql"  // Menggunakan MySQL sebagai provider
  url      = env("DATABASE_URL")  // URL koneksi diambil dari variabel lingkungan DATABASE_URL
}

generator client {
  provider = "prisma-client-js"
}

model TypeDoctor {
  id          Int       @id @default(autoincrement())
  name        String
  description String
  doctors     Doctor[]  @relation("DoctorType")
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}

model Doctor {
  id          Int          @id @default(autoincrement())
  name        String
  typeDoctors TypeDoctor[] @relation("DoctorType")
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
}

